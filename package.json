{
  "name": "bun-spark",
  "version": "0.0.1-alpha.4",
  "description": "(Spark) A query builder for Bun",
  "module": "index.ts",
  "type": "module",
  "main": "index.ts",
  "types": "index.ts",
  "scripts": {
    "dev": "bun run --watch index.ts",
    "test": "bun test test/",
    "test:coverage": "bun test --coverage",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --write",
    "format": "biome format . --write",
    "type-check": "tsc --noEmit",
    "build": "tsc",
    "check": "bun run test && bun run lint && bun run type-check"
  },
  "keywords": [
    "bun",
    "postgresql",
    "query-builder",
    "orm",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
